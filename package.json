{
  "name": "obd2",
  "version": "0.1.1",
  "description": "Vehicle communication and data parsing with OBD-II connector.",
  "main": "index.js",
  "scripts": {
    "start": "node example/server.js",
    "typings": "cd src && typings install",
    "lint": "tslint -c tslint.json \"src/**/*.ts\"",
    "docs": "typedoc --options typedoc.json ./src/",
    "build": "node bin/build",
    "test": "mocha --compilers ts:typescript-require --reporter spec test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sipimokus/node-obd2.git"
  },
  "keywords": [
    "obd",
    "obd2",
    "elm327",
    "bus",
    "can",
    "iso",
    "sae",
    "pwm",
    "kwp",
    "vpw",
    "pid",
    "at",
    "serial",
    "bluetooth",
    "usb",
    "car",
    "engine",
    "vehicle",
    "diagnostic",
    "diagnostics"
  ],
  "author": "Bence Sipos <sipimokus@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sipimokus/node-obd2/issues"
  },
  "homepage": "https://github.com/sipimokus/node-obd2",
  "dependencies": {
    "debug": "^2.2.0",
    "serialport": "^2.0.6"
  },
  "devDependencies": {
    "assert": "^1.3.0",
    "mocha": "^2.4.5",
    "tslint": "^3.3.0",
    "typedoc": "^0.3.12",
    "typescript": "^1.7.5",
    "typings": "^0.6.6"
  }
}
